apiVersion: v1
kind: Service
metadata:
  labels:
    app: dojot
  name: http-agent
  namespace: {{ dojot_namespace }}
spec:
  ports:
  - name: http-agent
    port: {{ dojot_http_agent_port }}
    targetPort: {{ dojot_http_agent_port }}
    protocol: TCP
  - name: http-agent-unsecure
    port: {{ dojot_http_agent_unsecure_port }}
    targetPort: {{ dojot_http_agent_unsecure_port }}    
  selector:
    name: http-agent
    app: dojot
apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    name: gui
    app: dojot
  name: gui
  namespace: {{ dojot_namespace }}
spec:
  replicas: 1
  selector:
    matchLabels:
      app: dojot
      name: gui
  template:
    metadata:
      labels:
        name: gui
        app: dojot
    spec:
      containers:
      - image: dojot/gui:{{ dojot_gui_version }}
        name: gui
      restartPolicy: Always

apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    name: device-manager-redis
    app: dojot
  name: device-manager-redis
  namespace: {{ dojot_namespace }}
spec:
  replicas: 1
  selector:
    matchLabels:
      app: dojot
      name: device-manager-redis
  template:
    metadata:
      labels:
        name: device-manager-redis
        app: dojot
    spec:
      containers:
      - image: dojot/redis:5.0.5-alpine3.10
        name: device-manager-redis
      restartPolicy: Always
